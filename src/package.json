{
  "name": "@ondewo/vtsi-client-angular",
  "version": "4.0.0",
  "description": "Ondewo voice telephone system integration client library for angular",
  "keywords": [
    "ondewo",
    "vtsi",
    "api",
    "client",
    "ngx-grpc",
    "grpc"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "submodule_update": "git submodule update --init --recursive",
    "build": "npm run generate",
    "generate": "docker run -it -v ${PWD}:/input-volume -v ${PWD}/..:/output-volume ondewo-angular-proto-compiler ondewo-vtsi-api ondewo",
    "debug": "docker run -it --entrypoint /bin/bash -v ${PWD}:/input-volume -v ${PWD}/..:/output-volume ondewo-angular-proto-compiler",
    "publish-npm": "npm publish ../npm --access public",
    "test-in-ondewo-aim": "cd .. && make build && cd src && rm -rf ../../ondewo-aim/node_modules/@ondewo/vtsi-client-angular/* && cp ../npm/* ../../ondewo-aim/node_modules/@ondewo/vtsi-client-angular/. -R"
  },
  "author": "ONDEWO GmbH",
  "homepage": "https://ondewo.com/",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@angular/common": ">=14.1.3",
    "@angular/core": ">=14.1.3",
    "@ngx-grpc/common": ">=3.0.2",
    "@ngx-grpc/core": ">=3.0.2"
  },
  "repository": {
    "type": "git",
    "directory": "https://github.com/ondewo/ondewo-vtsi-client-angular"
  }
}